CREATE OR REPLACE FUNCTION employer_details_func
    RETURN varchar;
 IS 
    var_salary varchar; 
 BEGIN 
	SELECT salary INTO var_salary FROM employee;
	RETURN var_salary;
 END;

e = employer_details_func();

dbms_output.put_line(var_float);
END



