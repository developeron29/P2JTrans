DECLARE
var_salary number;
var_string varchar;
var_float float;
BEGIN
SELECT salary INTO var_salary FROM employee ;
IF ( var_float > 3 ) 
THEN 
 dbms_output.put_line(var_salary); 
ELSE 
 dbms_output.put_line(var_string); 
END IF;

